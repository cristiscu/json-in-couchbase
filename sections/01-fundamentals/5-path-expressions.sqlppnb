{"cells":[{"kind":1,"language":"markdown","value":"see https://docs.couchbase.com/cloud/n1ql/n1ql-language-reference/nestedops.html"},{"kind":2,"language":"SQL++","value":"select *\r\nfrom [1, 2, 3] arr"},{"kind":2,"language":"SQL++","value":"select *\r\nfrom [[1, 2, 3]] arr"},{"kind":2,"language":"SQL++","value":"select *\r\nfrom [[1, 2, 3, { 'key': [1, 2, 3] }]] arr"},{"kind":2,"language":"SQL++","value":"select arr[1], arr[-1]\r\nfrom [[1, 2, 3, { 'key': [1, 2, 3] }]] arr"},{"kind":2,"language":"SQL++","value":"select arr[1:3], arr[1:], arr[:3]\r\nfrom [[1, 2, 3, { 'key': [1, 2, 3] }]] arr"},{"kind":2,"language":"SQL++","value":"select *\r\nfrom [1, 2, 3, { 'key': [1, 2, 3] }][1:3] arr"},{"kind":2,"language":"SQL++","value":"select arr[3]\r\nfrom [[1, 2, 3, { 'key': [1, 2, 3] }]] arr"},{"kind":2,"language":"SQL++","value":"select arr[3].`key`\r\nfrom [[1, 2, 3, { 'key': [1, 2, 3] }]] arr"},{"kind":2,"language":"SQL++","value":"select arr[3].[\"key\"] as `key`\r\nfrom [[1, 2, 3, { 'key': [1, 2, 3] }]] arr"},{"kind":2,"language":"SQL++","value":"select arr[3].[\"KEY\"]i as `key`\r\nfrom [[1, 2, 3, { 'key': [1, 2, 3] }]] arr"},{"kind":2,"language":"SQL++","value":"select arr[3][\"key\"]\r\nfrom [[1, 2, 3, { 'key': [1, 2, 3] }]] arr"},{"kind":2,"language":"SQL++","value":"select arr[3].key1[2]\r\nfrom [[1, 2, 3, { 'key1': [1, 2, 3] }]] arr"},{"kind":2,"language":"SQL++","value":"select arr[3].key1.child\r\nfrom [[1, 2, 3, { 'key1': { 'child': '234'} }]] arr"},{"kind":2,"language":"SQL++","value":"select\r\n    v.custom,\r\n    v.custom[0].name,\r\n    v.custom[1].valueAt,\r\n    v.custom[0].valueNum as vn1,\r\n    v.custom[0].[\"valueNum\"] as vn2,\r\n    TOSTRING(v.custom[0].[\"valueNum\"]) as vn3\r\n\r\nfrom\r\n{\r\n    \"custom\":\r\n    [\r\n        { \"name\": \"addressIdNum\",    \"valueNum\": 12345678},\r\n        { \"name\": \"cancelledDateAt\", \"valueAt\": \"2013-04-05 01:02:03\" }\r\n    ]\r\n} v;"}]}