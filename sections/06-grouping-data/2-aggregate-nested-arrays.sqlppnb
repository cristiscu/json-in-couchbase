{"cells":[{"kind":2,"language":"SQL++","value":"select\r\n    ARRAY_AVG([1, 2, 3, 2, null, 2]) as avg,\r\n    ARRAY_SUM([1, 2, 3, 2, null, 2]) as sum"},{"kind":2,"language":"SQL++","value":"select person.name,\r\n    ARRAY_AVG(person.ratings) as avg_ratings\r\nfrom [\r\n    { \"name\": 'John', \"ratings\": [3.5, 5.0, 4.5] },\r\n    { \"name\": 'Mary', \"ratings\": [5.0] },\r\n    { \"name\": 'Jack', \"ratings\": [4.8, 4.4, 4.9] }\r\n] person\r\nGROUP BY person.name, person.ratings"}]}