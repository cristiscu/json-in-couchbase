{"cells":[{"kind":2,"language":"SQL++","value":"select raw books\r\nfrom test.samples.books"},{"kind":2,"language":"SQL++","value":"UPDATE test.samples.books\r\nSET price = 24.99\r\nWHERE title = 'Sword of Honour'\r\nRETURNING raw books"},{"kind":2,"language":"SQL++","value":"select META().id as k, books as v\r\nfrom test.samples.books"},{"kind":2,"language":"SQL++","value":"UPDATE test.samples.books\r\nUSE KEYS \"12\"\r\nSET price = 54.99\r\n-- WHERE title = 'Sword of Honour'\r\nRETURNING raw books"},{"kind":2,"language":"SQL++","value":"UPDATE test.samples.store\r\nSET books = ARRAY_APPEND(books, {\"title\": \"This is a new book\"})\r\nRETURNING raw store.books"}]}