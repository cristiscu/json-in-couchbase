{"cells":[{"kind":2,"language":"SQL++","value":"select ratings\r\nfrom [3.5, 5.0, 4.5] ratings"},{"kind":2,"language":"SQL++","value":"select raw person\r\nfrom [{ \"name\": 'John' }, { \"name\": 'Mary' }, { \"name\": 'Jack' }] person"},{"kind":2,"language":"SQL++","value":"select person.*\r\nfrom [\r\n    { \"name\": 'John', \"ratings\": [3.5, 5.0, 4.5] },\r\n    { \"name\": 'Mary', \"ratings\": [5.0] },\r\n    { \"name\": 'Jack', \"ratings\": [4.8, 4.4, 4.9] }\r\n] person"},{"kind":2,"language":"SQL++","value":"select person.name, rating\r\nfrom [\r\n    { \"name\": 'John', \"ratings\": [3.5, 5.0, 4.5] },\r\n    { \"name\": 'Mary', \"ratings\": [5.0] },\r\n    { \"name\": 'Jack', \"ratings\": [4.8, 4.4, 4.9] }\r\n] person\r\nUNNEST person.ratings as rating"},{"kind":2,"language":"SQL++","value":"select person.name, AVG(rating) as ratings\r\nfrom [\r\n    { \"name\": 'John', \"ratings\": [3.5, 5.0, 4.5] },\r\n    { \"name\": 'Mary', \"ratings\": [5.0] },\r\n    { \"name\": 'Jack', \"ratings\": [4.8, 4.4, 4.9] }\r\n] person\r\nUNNEST person.ratings as rating\r\ngroup by person.name\r\norder by person.name"},{"kind":2,"language":"SQL++","value":"select person.name, ARRAY_AGG(rating) as ratings\r\nfrom [\r\n    { \"name\": 'John', \"ratings\": [3.5, 5.0, 4.5] },\r\n    { \"name\": 'Mary', \"ratings\": [5.0] },\r\n    { \"name\": 'Jack', \"ratings\": [4.8, 4.4, 4.9] }\r\n] person\r\nUNNEST person.ratings as rating\r\ngroup by person.name\r\norder by person.name"},{"kind":2,"language":"SQL++","value":"select person.name, ARRAY_AGG(rating) as ratings\r\nfrom [\r\n\t{ \"name\": \"John\", \"rating\": 3.5 },\r\n\t{ \"name\": \"John\", \"rating\": 5   },\r\n\t{ \"name\": \"John\", \"rating\": 4.5 },\r\n\t{ \"name\": \"Mary\", \"rating\": 5   },\r\n\t{ \"name\": \"Jack\", \"rating\": 4.8 },\r\n\t{ \"name\": \"Jack\", \"rating\": 4.4 },\r\n\t{ \"name\": \"Jack\", \"rating\": 4.9 }\r\n] person\r\n-- UNNEST person.ratings as rating\r\ngroup by person.name\r\norder by person.name