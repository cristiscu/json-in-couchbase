{"cells":[{"kind":2,"language":"SQL++","value":"select ARRAY_MIN(arr) as min,\r\n    ARRAY_MAX(arr) as max,\r\n    ARRAY_SUM(arr) as sum,\r\n    ARRAY_AVG(arr) as avg\r\nfrom [[1, 2, 3, 2, null, 2]] arr"},{"kind":2,"language":"SQL++","value":"select *\r\nfrom [1, 2, 3, 2, null, 2] arr"},{"kind":2,"language":"SQL++","value":"select MIN(arr) as min,\r\n    MAX(arr) as max,\r\n    SUM(arr) as sum,\r\n    AVG(arr) as avg\r\nfrom [1, 2, 3, 2, null, 2] arr"},{"kind":2,"language":"SQL++","value":"select ARRAY_AGG(arr) as `all`,\r\n    ARRAY_AGG(DISTINCT arr) as `distinct`\r\nfrom [1, 2, 3, 2, null, 2] arr"},{"kind":2,"language":"SQL++","value":"select *\r\nfrom [\r\n    { \"name\": \"John\", \"age\": 25 },\r\n    { \"name\": \"Mary\", \"age\": 19 },\r\n    { \"name\": \"Jack\", \"age\": 33 }] arr"},{"kind":2,"language":"SQL++","value":"select raw ARRAY_AGG(arr)\r\nfrom [\r\n    { \"name\": \"John\", \"age\": 25 },\r\n    { \"name\": \"Mary\", \"age\": 19 },\r\n    { \"name\": \"Jack\", \"age\": 33 }] arr"},{"kind":2,"language":"SQL++","value":"select ARRAY_AGG(arr.name) as names, \r\n    ARRAY_AGG(arr.age) as ages\r\nfrom [\r\n    { \"name\": \"John\", \"age\": 25 },\r\n    { \"name\": \"Mary\", \"age\": 19 },\r\n    { \"name\": \"Jack\", \"age\": 33 }] arr"}]}