{"cells":[{"kind":2,"language":"SQL++","value":"select ARRAY_REPEAT(\"abc\", 3) as repeat,\r\n    ARRAY_RANGE(1, 7, 2) as `range`"},{"kind":2,"language":"SQL++","value":"select SPLIT('This is something!'),\r\n    SPLIT('abc, 1, null', ', ')"},{"kind":2,"language":"SQL++","value":"select CONCAT2(' ', 'This', 'is', 'something!'),\r\n    CONCAT2(' ', ['This', 'is', 'something!']),\r\n    CONCAT2(' ', SPLIT('This is something!'))"},{"kind":2,"language":"SQL++","value":"select ARRAY_SORT([1, 3, 2, 3]) as sort,\r\n    ARRAY_REVERSE([1, 3, 2, 3]) as reverse,\r\n    ARRAY_DISTINCT([1, 3, 2, 3]) as `distinct`"},{"kind":2,"language":"SQL++","value":"select raw ARRAY_STAR([{ \"k1\": 1, \"k2\": 2 }, { \"k1\": 1, \"k3\": 3 }])"},{"kind":2,"language":"SQL++","value":"select ARRAY_FLATTEN([[1], [2, [3, 4]]], 1) as flatten1,\r\n    ARRAY_FLATTEN([[1], [2, [3, 4]]], 2) as flatten2"}]}