{"cells":[{"kind":2,"language":"SQL++","value":"select OBJECT_CONCAT(obj, { \"k3\": null }, { \"k2\": 123 }, { \"k2\": [1, 2, 3]})\r\nfrom { \"k1\": 1, \"k2\": \"abc\" } obj"},{"kind":2,"language":"SQL++","value":"select OBJECT_ADD(obj, \"k3\", null), OBJECT_ADD(obj, \"k2\", null),\r\n    OBJECT_PUT(obj, \"k3\", null), OBJECT_PUT(obj, \"k2\", null)\r\nfrom { \"k1\": 1, \"k2\": \"abc\" } obj"},{"kind":2,"language":"SQL++","value":"select OBJECT_REPLACE(obj, \"abc\", \"123\")\r\nfrom { \"k1\": 1, \"k2\": \"abc\", \"k3\": \"abc\" } obj"},{"kind":2,"language":"SQL++","value":"select OBJECT_RENAME(obj, \"k2\", \"k2_new\")\r\nfrom { \"k1\": 1, \"k2\": \"abc\" } obj"},{"kind":2,"language":"SQL++","value":"select OBJECT_REMOVE(obj, \"k2\")\r\nfrom { \"k1\": 1, \"k2\": \"abc\" } obj"},{"kind":2,"language":"SQL++","value":"select OBJECT_REMOVE({ \"k1\": 1, \"k2\": \"abc\" }, \"k2\")"}]}