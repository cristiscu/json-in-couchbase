{"cells":[{"kind":2,"language":"SQL++","value":"select ARRAY_APPEND(['a', 'b'], 1, 2) as append,\r\n    ARRAY_PREPEND(1, 2, ['a', 'b']) as prepend,\r\n    ARRAY_INSERT(['a', 'b'], 1, 1, 2) as `insert`"},{"kind":2,"language":"SQL++","value":"select ARRAY_REPLACE([1, 2, 3, 2], 2, 4, 1) as one,\r\n    ARRAY_REPLACE([1, 2, 3, 2], 2, 4) as `all`"},{"kind":2,"language":"SQL++","value":"select ARRAY_PUT([1, 2, 3], 2, 4, 4)"},{"kind":2,"language":"SQL++","value":"select ARRAY_MOVE(['a', 'b', 'c'], 1, 2)"},{"kind":2,"language":"SQL++","value":"select ARRAY_REMOVE([1, 2, 2, 3, 4, 4, 5], 6, 2, 1)"}]}