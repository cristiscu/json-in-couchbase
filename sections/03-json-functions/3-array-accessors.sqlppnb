{"cells":[{"kind":2,"language":"SQL++","value":"select *\r\nfrom [1, 2, 3] arr"},{"kind":2,"language":"SQL++","value":"select a.*\r\nfrom (select [1, 2, 3] as arr) a"},{"kind":2,"language":"SQL++","value":"select *\r\nfrom [[1, 2, 3]] arr"},{"kind":2,"language":"SQL++","value":"select a.arr\r\nfrom { \"arr\": [1, 2, 3] } a"},{"kind":2,"language":"SQL++","value":"select ARRAY_LENGTH(a.arr) as length,\r\n    ARRAY_COUNT(a.arr) as count,\r\n    ARRAY_IFNULL(a.arr) as ifnull\r\nfrom { \"arr\": [null, null, 'abc'] } a"},{"kind":2,"language":"SQL++","value":"select ARRAY_CONTAINS(a.arr, 'abc') as contains,\r\n    ARRAY_POSITION(a.arr, 'abc') as position,\r\n    ARRAY_BINARY_SEARCH(a.arr, 'abc') as search\r\nfrom { \"arr\": [null, null, 'abc'] } a"}]}