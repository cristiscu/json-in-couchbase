{"cells":[{"kind":2,"language":"SQL++","value":"select raw b\r\nfrom test.samples.store s\r\nunnest s.books b"},{"kind":2,"language":"SQL++","value":"DROP SEQUENCE test.samples.book_seq IF EXISTS"},{"kind":2,"language":"SQL++","value":"CREATE SEQUENCE test.samples.book_seq\r\n    START WITH 10\r\n    INCREMENT BY 2\r\n    MAXVALUE 10000\r\n    MINVALUE 0\r\n    CYCLE"},{"kind":2,"language":"SQL++","value":"DROP COLLECTION test.samples.books IF EXISTS"},{"kind":2,"language":"SQL++","value":"CREATE COLLECTION test.samples.books IF NOT EXISTS"},{"kind":2,"language":"SQL++","value":"INSERT INTO test.samples.books (KEY k, VALUE v)\r\nselect TO_STRING(NEXT VALUE FOR test.samples.book_seq) AS k, b AS v\r\nfrom test.samples.store s\r\nunnest s.books b\r\nRETURNING META().id as k, *;"},{"kind":2,"language":"SQL++","value":"select META().id as `key`, b as `value`\r\nfrom test.samples.books b\r\norder by `key`"},{"kind":2,"language":"SQL++","value":"select raw b\r\nfrom test.samples.books b"}]}