{"cells":[{"kind":2,"language":"SQL++","value":"select raw b\r\nfrom test.samples.store s\r\nunnest s.books b"},{"kind":2,"language":"SQL++","value":"DROP COLLECTION test.samples.books IF EXISTS"},{"kind":2,"language":"SQL++","value":"CREATE COLLECTION test.samples.books IF NOT EXISTS"},{"kind":2,"language":"SQL++","value":"INSERT INTO test.samples.books (KEY k, VALUE v)\r\nselect UUID() AS k, b AS v\r\nfrom test.samples.store s\r\nunnest s.books b\r\nRETURNING META().id as k, *;"},{"kind":2,"language":"SQL++","value":"select META().id as `key`, b as `value`\r\nfrom test.samples.books b"},{"kind":2,"language":"SQL++","value":"select raw b\r\nfrom test.samples.books b"}]}