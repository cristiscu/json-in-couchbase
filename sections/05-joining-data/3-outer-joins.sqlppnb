{"cells":[{"kind":2,"language":"SQL++","value":"with persons as (\r\n    select p.* from [\r\n        { \"id\": 1, \"name\": 'John' },\r\n        { \"id\": 2, \"name\": 'Mary' },\r\n        { \"id\": 3, \"name\": 'Jack' }] p),\r\n\r\norders as (\r\n    select o.* from [\r\n        { \"total\": 100, \"pid\": 1 },\r\n        { \"total\": 200, \"pid\": 1 },\r\n        { \"total\": 150, \"pid\": 2 }] o)\r\n\r\nselect p.name, o orders\r\nfrom persons p LEFT JOIN orders o ON p.id = o.pid"},{"kind":2,"language":"SQL++","value":"with persons as (\r\n    select p.* from [\r\n        { \"id\": 1, \"name\": 'John' },\r\n        { \"id\": 2, \"name\": 'Mary' },\r\n        { \"id\": 3, \"name\": 'Jack' }] p),\r\n\r\norders as (\r\n    select o.* from [\r\n        { \"total\": 100, \"pid\": 1 },\r\n        { \"total\": 200, \"pid\": 1 },\r\n        { \"total\": 150, \"pid\": 2 }] o)\r\n\r\nselect p.name, o orders\r\nfrom orders o RIGHT OUTER JOIN persons p ON p.id = o.pid"}]}