{"cells":[{"kind":2,"language":"SQL++","value":"CREATE OR REPLACE FUNCTION celsius1(arg)\r\n{ (SELECT RAW ((arg - 32) * 5/9))[0] }"},{"kind":2,"language":"SQL++","value":"EXECUTE FUNCTION celsius1(100)"},{"kind":2,"language":"SQL++","value":"EXPLAIN FUNCTION celsius1"},{"kind":2,"language":"SQL++","value":"DROP FUNCTION test.samples.celsius2 IF EXISTS"},{"kind":2,"language":"SQL++","value":"CREATE FUNCTION test.samples.celsius2(arg) IF NOT EXISTS\r\n{ (arg - 32) * 5/9 }"},{"kind":2,"language":"SQL++","value":"EXECUTE FUNCTION test.samples.celsius2(100)"},{"kind":2,"language":"SQL++","value":"DROP FUNCTION test.samples.celsius3"},{"kind":2,"language":"SQL++","value":"CREATE OR REPLACE FUNCTION test.samples.celsius3(...)\r\nLANGUAGE INLINE AS\r\n(args[0] - 32) * 5/9"},{"kind":2,"language":"SQL++","value":"EXECUTE FUNCTION test.samples.celsius3(100)"},{"kind":2,"language":"SQL++","value":"DROP FUNCTION celsius4"},{"kind":2,"language":"SQL++","value":"CREATE FUNCTION celsius4(arg)\r\nLANGUAGE JAVASCRIPT AS\r\n\"function celsius4(p1) { return (p1 - 32) * 5/9; }\""},{"kind":2,"language":"SQL++","value":"EXECUTE FUNCTION celsius4(100)"},{"kind":2,"language":"SQL++","value":"SELECT celsius1(100) as celsius1,\r\n    test.samples.celsius2(100) as celsius2,\r\n    test.samples.celsius3(100) as celsius3,\r\n    celsius4(100) as celsius4"},{"kind":2,"language":"SQL++","value":"select f.*\r\nfrom system:functions f"}]}